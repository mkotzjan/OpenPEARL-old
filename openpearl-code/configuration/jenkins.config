#
# Automatically generated make config: don't edit
#

#
# Compiler
#
CONFIG_COMPILER_ANTLR="/usr/local/lib/antlr-4.5-complete.jar"

#
# Runtime System
#
# CONFIG_CANSUPPORT is not set
CONFIG_HAS_I2C=y

#
# I2C-Components
#
# CONFIG_I2C_LM75 is not set
# CONFIG_I2C_PCF8574 is not set
CONFIG_LINUX=y
# CONFIG_LINUX_Raspberry is not set
# CONFIG_LINUX_tests is not set

#
# Drivers
#

#
# The standard drivers are build automatically 
#

#
# --> StdStream, Disc, Pipe                    
#
# CONFIG_LINUX_Octopus is not set
# CONFIG_LINUX_I2C is not set
CONFIG_LPC1768=y
CONFIG_LPC1768_CHECK_STACK_OVERFLOW=y
# CONFIG_LPC1768_tests is not set

#
# Drivers
#
CONFIG_LPC1768_HAS_USB=y
CONFIG_LPC1768_USB_Keyboard=y
CONFIG_LPC1768_USB_Disk=y
CONFIG_LPC1768_UART=y
CONFIG_LPC1768_HY32A=y
# CONFIG_DOC_PREPORT is not set

#
# Installation Settings
#
CONFIG_INSTALL_Target="/usr/local"
CONFIG_INSTALL_FLASHINTERFACE="/dev/ttyUSB0"
